<link rel="stylesheet" type="text/css" href="~/static/h-ui.admin/css/H-ui.admin.css" />
<link rel="stylesheet" type="text/css" href="~/lib/Hui-iconfont/1.0.8/iconfont.css" />
<link rel="stylesheet" type="text/css" href="~/static/h-ui.admin/skin/default/skin.css" id="skin" />
<link rel="stylesheet" type="text/css" href="~/static/h-ui.admin/css/style.css" />
<!--[if IE 6]>
<script type="text/javascript" src="lib/DD_belatedPNG_0.0.8a-min.js" ></script>
<script>DD_belatedPNG.fix('*');</script>
<![endif]-->
<title>预约信息- 预约信息查看</title>
</head>
<body>
<article class="page-container">
	<form class="form form-horizontal" id="form-admin-add">

        <div style="border:2px solid #0B96CD;font-weight:bold;border-radius:6px;">
            <div class="row cl" style="margin-top:0px;">
		        <label class="form-label col-xs-4 col-sm-3">预约日期：</label>
		        <div class="formControls col-xs-8 col-sm-9 " style="margin-top:3px;">
                    @Model.Sdate.Date.ToShortDateString()
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">预约时段：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @if (Model.DateFlag == 1) { 
                        <span>上午第1、2节课</span>
                    }
                    else if (Model.DateFlag == 2)
                    {
                        <span>上午第3、4节课</span>
                    }
                    else if (Model.DateFlag == 3)
                    {
                        <span>下午课程时段</span>
                    }
                    else if (Model.DateFlag == 4)
                    {
                        <span>晚自习时段</span>
                    }
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">主要用途：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Useinfo
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">参考收费：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Money 元
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">下单时间：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Rdate
		        </div>
	        </div>

        </div>


        <div style="border:2px solid #0B96CD;font-weight:bold;border-radius:6px;margin-top:6px;">   
	        <div class="row cl" style="margin-top:0px;">
		        <label class="form-label col-xs-4 col-sm-3">预约人姓名：</label>
		        <div class="formControls col-xs-8 col-sm-9 " style="margin-top:3px;">
                    @Model.Userinfo.Uname
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">预约人学号：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Userinfo.Number 
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">联系方式：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Userinfo.Phone
		        </div>
	        </div>

            <div class="row cl" style="margin-bottom:5px;">
		        <label class="form-label col-xs-4 col-sm-3">宿舍地址：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Userinfo.Address
		        </div>
	        </div>
        </div> 

        <div style="border:2px solid #0B96CD;font-weight:bold;border-radius:6px;margin-top:6px;">
            <div class="row cl" style="margin-top:0px;">
		        <label class="form-label col-xs-4 col-sm-3">电脑编号：</label>
		        <div class="formControls col-xs-8 col-sm-9 " style="margin-top:3px;">
                    @Model.Computerinfo.Number
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">所在排：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Row
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">所在列：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Col
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">品牌型号：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Brand @Model.Computerinfo.Model
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">处理器：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.CPU
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">内存：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Memory
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">费用：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Money 元/小时
		        </div>
	        </div>

        </div>
    
        <div style="border:2px solid #0B96CD;font-weight:bold;border-radius:6px;margin-top:6px;">
            <div class="row cl" style="margin-top:0px;">
		        <label class="form-label col-xs-4 col-sm-3">所属机房号：</label>
		        <div class="formControls col-xs-8 col-sm-9 " style="margin-top:3px;">
                    @Model.Computerinfo.Machineroom.Number
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">机房名：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Machineroom.Name
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">机房地址：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Machineroom.Address
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">电脑总数：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Machineroom.Count
		        </div>
	        </div>

        </div>

        <div style="border:2px solid #0B96CD;font-weight:bold;border-radius:6px;margin-top:6px;">
            <div class="row cl" style="margin-top:0px;">
		        <label class="form-label col-xs-4 col-sm-3">机房管理员：</label>
		        <div class="formControls col-xs-8 col-sm-9 " style="margin-top:3px;">
                    @Model.Computerinfo.Machineroom.Userinfo.Uname
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">工号：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Machineroom.Userinfo.Number
		        </div>
	        </div>

            <div class="row cl">
		        <label class="form-label col-xs-4 col-sm-3">联系方式：</label>
		        <div class="formControls col-xs-8 col-sm-9" style="margin-top:3px;">
                    @Model.Computerinfo.Machineroom.Userinfo.Phone
		        </div>
	        </div>
        </div>

    
	</form>
</article>

<!--_footer 作为公共模版分离出去--> 
